{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "jest"],
  "rules": {
    "camelcase": 0,
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": false,
    "no-await-in-loop": 0,
    "no-loop-func": 0
  },
  "env": {
    "jest/globals": true
  }
}
